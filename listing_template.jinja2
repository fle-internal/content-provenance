<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title>{{title}}</title>
	<link rel="stylesheet" href="style.css" type="text/css">
</head>
<body>
	<h1>{{title}}</h1>
  
	<p>{{description}}</p>
	
	<hr/>
  {% for org, channels_info in sample_channels_info.items() %}
     <h2>{{org}} channels</h2>
     
     <ul>
     {% for channel_id, channel_name in channels_info.items() %}
        <li>
          <a href="./{{channel_id}}/">{{channel_name}} ({{channel_id}})</a>
        </li>
     {% endfor %}
     </ul>

     <hr/>
  {% endfor %}
  
</body>
</html>
